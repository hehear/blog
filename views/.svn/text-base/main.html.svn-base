
<div class="contentpanel" id='contentpanel' >
    <div class="row" >

        <div class="col-sm-6 col-md-3" v-for="(item,index) in targetTpInfoList" >

            <div v-bind:class="item.color" v-on:click="toDtls(item)">
                <div class="panel-heading">

                    <!--<div class="stat" data-toggle="dropdown">-->
                    <div class="stat" >
                        <div class="row">
                            <div class="col-xs-4">
                                <img src="/kj/images/is-document.png" alt="" />
                            </div>
                            <div class="col-xs-8">
                                <small class="stat-label">当前任务</small>
                                <h2>${item.target_tp}：${item.count||0}个</h2>
                            </div>
                        </div><!-- row -->

                        <div class="mb15"></div>

                        <div class="row">
                            <div class="col-xs-6">
                                <small class="stat-label">完成</small>
                                <h4>${item.complete_count||0}</h4>
                            </div>

                            <div class="col-xs-6">
                                <small class="stat-label">% 总进度</small>
                                <h4>0.43%</h4>
                            </div>
                        </div><!-- row -->
                    </div><!-- stat -->


                </div><!-- panel-heading -->
            </div><!-- panel -->
        </div><!-- col-sm-6 -->


    </div><!-- row -->


    <div id ="myModal" class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">

        <div class="modal-dialog modal-sm" role="document">
            <div class="modal-content">
                <div class="modal-header" >
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h5 class="modal-title" >${tartgetTp}任务</h5>
                </div>
                <ul  style="border: 1px solid #ddd;border-top: 1;padding: 0 10px;" >

                    <h5 class="title">&nbsp;今天有 ${targetDtlsCount} 个${tartgetTp}任务：</h5>
                </ul>
                <!--modal-body -->
                <div class="modal-body">

                        <ul class="dropdown-list gen-list" style="border: 1px solid #ddd;border-top: 1;padding: 0 10px;" v-for="(item,index) in targetDtlsList" >

                        <li class="new">
                        <a>
                        <span class="thumb">任务${index+1}</span>
                        <span class="desc">
                        <span class="name">${item.target_tp}
                            <span class="badge badge-warning " v-if="item.complete_st==0" v-on:click="toSetCompleteSt(item,1)">打卡</span>
                            <span class="badge badge-danger " v-if="item.complete_st==1">进行中</span>
                            <span class="badge badge-success " v-if="item.complete_st==2">完成</span>
                            <span class="badge badge-fail " v-if="item.complete_st==3">未完成</span>
                        </span>
                        <span class="msg">${item.target_content}</span>
                            <textarea class="msg" v-model="item.target_complete_content" style="width:80%;margin-top: 6px;margin-bottom: 4px; display: inline;border-style:solid;border-color:black" placeholder="完成情况..." v-if="item.complete_st==1"></textarea>
                            <div v-if="item.complete_st==1">
                                <button v-on:click="toSetCompleteSt(item,2)">完成</button>
                                <button v-on:click="toSetCompleteSt(item,3)">未完成</button>
                            </div>

                        </span>

                        </a>
                        </li>

                        </ul>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">关闭</button>
                </div>
            </div>
        </div>
    </div>
    <!--  新增修改模态框 end-->

    <div class="row" v-if="targetTpInfoList.length==0">
