<script>
    $(function(){
        $('.js-menu-item').click(function(){
            /* console.log($(this).data('url'));*/
            //等待框
            //var $waitModal=$.com_modal_wait.show();
            var parent=$(this).parents('.nav-parent:first').find('span:first').text();
            var child=$(this).text();
            $("#currposition").html(parent+'&nbsp;/&nbsp;'+child);
            $.ajax({
                url:$(this).data('url')||'',
                type:'get',
                data:{},
                success:function(rs){
                    //$waitModal.close();
                    $('#contentpanel').html(rs);

                }
            });
        });
    });

    //从cookiez中读取当前登录账户
    $(function(){
        let uid=getCookie('uid');
        $("#js-currentuser").text(uid);
    });

</script>

<script>

    var vm_index = new Vue({
        el:'#vm_index',
        delimiters: ['${', '}'],
        data:{
            infoCount:0,
            curr:{},
            //结果信息
            infolist:[],
            //任务分类列表
            targetTpInfoList:[],
            //任务明细列表
            targetDtlsList:[],
            //任务明细个数
            targetDtlsCount:0,
            //任务类型
            tartgetTp:'',
            //任务完成情况说明
            targetCompleteContent:'',
            //当前用户
            uid:'admin',
            //任务颜色
            colorList:[{color:'panel panel-success panel-stat',img:'/kj/images/is-document.png'},
                {color:'panel panel-danger panel-stat',img:'/kj/images/is-user.png'},
                {color:'panel panel-primary panel-stat',img:'/kj/images/is-money.png'},
                {color:'panel panel-dark panel-stat',img:'/kj/images/is-document.png'}]
        },
        methods:{
            reset:function(){
                vm.filter=vm.defaultFilter;
            },
            loadInfoList:function(){

                console.log(document.cookie);

                $.ajax({
                    url:'/rest/info/queryInfo',
                    type:'get',
                    data:{},
                    success:function(rs){

                        if(rs &&( rs.code==200)){

                            //消息条数
                            vm_index.infoCount=rs.page;

                            //赋值消息结果信息
                            vm_index.infolist=rs.rs;


                        }else{
                            console.log('----加载失败-----');
                            // vm.rsMes=rs.message;
                            // vm.pager={};
                        }
                    }
                });
            },
            loadtargetList:function(){

                $.ajax({
                    url:'/rest/target/queryTargets',
                    type:'get',
                    data:{},
                    success:function(rs){

                        if(rs &&( rs.code==200)){

                            //赋值消息结果信息
                            for (var i=0;i<rs.rs.length; i++){
                                rs.rs[i].color=vm_index.colorList[i].color;
                                rs.rs[i].img=vm_index.colorList[i].img;
                            }
                            vm_index.targetTpInfoList=rs.rs;


                        }else{
                            console.log('----加载失败-----');
                            // vm.rsMes=rs.message;
                            // vm.pager={};
                        }
                    }
                });
            },
            //打卡
            toSetCompleteSt:function(item,st){

                item.complete_st=st;

                //提交完成或者失败状态时提交原因
                if(st==2||st==3){
                    //有内容时提交
                    if(item.target_complete_content){

                        $.ajax({
                            url:'/rest/target/updtCompleteContent',
                            type:'post',
                            data:item,
                            success:function(rs){

                                if(rs &&( rs.code==200)){

                                    console.log('修改完成内容说明成功！')
                                }else{
                                    console.log('----加载失败-----');

                                }
                            }
                        });

                    }


                }

                $.ajax({
                    url:'/rest/target/updtCompleteSt',
                    type:'post',
                    data:item,
                    success:function(rs){

                        if(rs &&( rs.code==200)){


                            $.com_modal_info.show(rs.rs);

                            vm_index.toDtls(item);


                        }else{
                            console.log('----加载失败-----');
                            vm.rsMes=rs.message;
                            vm.pager={};
                        }
                    }
                });

                $('#myModal').modal('show');
            },
            //读消息
            toRead:function(item){

                if(item.read_st=='1'){
                    return
                }

                $.ajax({
                    url:'/rest/info/updtReadSt',
                    type:'post',
                    data:item,
                    success:function(rs){

                        if(rs &&( rs.code==200)){

                            vm_index.loadInfoList();


                        }else{
                            console.log('----加载失败-----');
                            vm.rsMes=rs.message;
                            vm.pager={};
                        }
                    }
                });
            },
            //所有消息已读
            toReadAll:function(){


                $.ajax({
                    url:'/rest/info/updtReadSt',
                    type:'post',
                    data:{},
                    success:function(rs){

                        if(rs &&( rs.code==200)){


                            vm_index.loadInfoList();


                        }else{
                            console.log('----加载失败-----');
                            vm.rsMes=rs.message;
                            vm.pager={};
                        }
                    }
                });
            },

            //获得当前登陆用户
            getCurrentUser:function(){

                $.ajax({
                    url:'/getCurrentUser',
                    type:'get',
                    data:{},
                    success:function(rs){

                        if(rs &&( rs.code==200)){

                            vm_index.uid=rs.rs;

                        }else{
                            console.log('----加载失败-----');
                            vm.rsMes=rs.message;
                            vm.pager={};
                        }
                    }
                });
            },

            dateFormat:function() {

                var date = new Date();
                var year=date.getFullYear();
                var month= date.getMonth()+1<10 ? "0"+(date.getMonth()+1) : date.getMonth()+1;
                var day=date.getDate()<10 ? "0"+date.getDate() : date.getDate();

                var hours=date.getHours()<10 ? "0"+date.getHours() : date.getHours();
                var minutes=date.getMinutes()<10 ? "0"+date.getMinutes() : date.getMinutes();
                var seconds=date.getSeconds()<10 ? "0"+date.getSeconds() : date.getSeconds();
                // 拼接
                return year+"-"+month+"-"+day+" "+hours+":"+minutes+":"+seconds;
            },
            //获得当前登陆用户
            toSleep:function(){

                var filter =  {
                    sleep_tm:vm_index.dateFormat()

                };

                $.ajax({
                    url:'/rest/day/updt',
                    type:'post',
                    data:filter,
                    success:function(rs){

                        if(rs &&( rs.code==200)){

                            $.com_modal_info.show({title:'温馨提示',content:'晚安，好好休息吧！！！！',callback:function(){
                                    window.location.href="/sleep";
                            }});


                        }else{
                            console.log('----加载失败-----');
                            vm.rsMes=rs.message;
                            vm.pager={};
                        }
                    }
                });
            },


            //任务明细
            toDtls:function(item){

                vm_index.tartgetTp=item.target_tp;

                $.ajax({
                    url:'/rest/target/queryTargetsByTp',
                    type:'post',
                    data:{target_tp:item.target_tp},
                    success:function(rs){

                        if(rs &&( rs.code==200)){

                            //消息条数
                            vm_index.targetDtlsCount=rs.rs.length;

                            //赋值消息结果信息
                            vm_index.targetDtlsList=rs.rs;


                        }else{
                            console.log('----加载失败-----');
                            vm.rsMes=rs.message;
                            vm.pager={};
                        }
                    }
                });

                $('#myModal').modal('show');
            }


        }
    });

    vm_index.loadtargetList();
    vm_index.loadInfoList();
    vm_index.getCurrentUser();


</script>


</html>
